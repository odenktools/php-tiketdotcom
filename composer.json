{
  "name": "odenktools/php-tiketdotcom",
  "type": "library",
  "description": "Library REST API Untuk Develop tiket.com",
  "keywords": [
      "hotel",
      "flight",
      "http",
      "rest",
      "php",
      "Tiket.com",
      "HTTP client"
  ],
  "homepage": "https://github.com/odenktools/php-tiketdotcom",
  "license": "MIT",
  "authors": [{
    "name": "odenktools",
    "email": "odenktools86@gmail.com",
    "homepage": "https://odenktools.com",
    "role": "Developer"
  }],
  "require": {
    "php": ">=5.4",
    "ext-curl": "*",
    "mashape/unirest-php": "3.0.4",
    "nesbot/carbon": "~1.18"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0"
  },
  "autoload": {
    "psr-0": {
      "Tiket\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit --colors=always",
    "test:ci": "composer test -- --verbose --coverage-text --coverage-clover=coverage.xml"
  }
}
